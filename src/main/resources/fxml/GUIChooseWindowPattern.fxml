                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
        <GridPane maxHeight="709.0" minHeight="328.0" minWidth="960.0" prefHeight="328.0" prefWidth="960.0" stylesheets="@../css/style.css" VBox.vgrow="ALWAYS">
