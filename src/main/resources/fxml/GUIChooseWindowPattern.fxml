            <GUIWindowPattern fx:id = "windowTwo" onMouseClicked="#selectSecond" GridPane.columnIndex="2" GridPane.columnSpan="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.rowSpan="1" GridPane.vgrow="ALWAYS">
