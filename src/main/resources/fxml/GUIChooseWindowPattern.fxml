<?import it.polimi.se2018.view.gui.fxmlController.GUIWindowPattern?>
<?import it.polimi.se2018.view.gui.fxmlController.GUIPrivateObjectiveCard?>
<VBox minHeight="540.0" minWidth="960.0" prefHeight="540.0" prefWidth="960.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.se2018.view.gui.fxmlController.GUIChooseWindowPattern">
